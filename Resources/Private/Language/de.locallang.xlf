                <source>Allow cookies</source>
                <target state="translated">Cookies erlauben</target>
            </trans-unit>
            <trans-unit id="cookieconsent.common.deny">
                <source>Decline</source>
                <target state="translated">Ablehnen</target>
            </trans-unit>
            <trans-unit id="cookieconsent.common.link">
                <source>Learn more</source>
                <target state="translated">Mehr erfahren</target>
