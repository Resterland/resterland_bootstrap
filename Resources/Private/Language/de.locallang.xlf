        <body>

            <!-- Logo Settings Header -->
            <trans-unit id="constant_cats.logo" xml:space="preserve">
                <source>Logo</source>
                <target>Logo</target>
            </trans-unit>

            <!-- Logo File Path -->
            <trans-unit id="constant_cats.logo.file" xml:space="preserve">
                <source>Path to the Logo: Choose one of the list or submit your own</source>
                <target>Pfad zum Logo: Wähle eines aus der Liste oder gib den gewünschten Pfad an</target>
            </trans-unit>

